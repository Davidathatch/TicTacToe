@using System.Diagnostics
@using TicTacToe.Components
@using TicTacToe.Models

@page "/play"

<PageTitle>Home</PageTitle>

<div id="home-page">
    <img id="bg-image-tl" class="bg-image" src="Assets/Squiggle.svg"/>
    <img id="bg-image-tr" class="bg-image" src="Assets/Donut.svg"/>
